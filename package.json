{
  "name": "hayden",
  "version": "1.0.0",
  "description": "An app for Hayden's website",
  "main": "lambda.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 --profile hayden",
