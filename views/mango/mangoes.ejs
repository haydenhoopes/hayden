    <table class="table small">
        <thead>
            <tr>
                <th class="d-none d-sm-none d-md-table-cell">ID</th>
                <th>Date</th>
                <th>Source</th>
                <th>Category</th>
                <th class="d-none d-sm-none d-md-table-cell">Type</th>
                <th class="d-none d-sm-none d-md-table-cell">Description</th>
                <th class="d-none d-sm-none d-md-table-cell">Payment Method</th>
                <th class="d-none d-sm-none d-md-table-cell">Is Normal?</th>
                <th class="d-none d-sm-none d-md-table-cell">Who?</th>
                <th>Amount</th>
            </tr>
        </thead>
        <tbody>
            <% let count = 1; %>
            <% mangoes.map(mango => { %>
                <a href="/mangoes/<%= mango._id %>"><tr>
                    <td class="d-none d-sm-none d-md-table-cell"><%= count %></td>
                    <td><%= mango.date %></td>
                    <td><%= mango.source %></td>
                    <td><%= mango.category %></td>
                    <td class="d-none d-sm-none d-md-table-cell"><%= mango.type %></td>
                    <td class="d-none d-sm-none d-md-table-cell"><%= mango.description %></td>
                    <td class="d-none d-sm-none d-md-table-cell"><%= mango.paymentMethod %></td>
                    <td class="d-none d-sm-none d-md-table-cell"><%= mango.isNormal %></td>
                    <td class="d-none d-sm-none d-md-table-cell"><%= mango.who %></td>
                    <td>$ <%= mango.amount %></td>
                </tr></a>
                <% count++; %>
            <% }) %> 
        </tbody>
    </table>
</div>